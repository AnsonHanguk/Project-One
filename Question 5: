public class helloWorld {
	 String helloWorld;
	public static void main(String[] args) {
		helloWorld myObject = new helloWorld();
		myObject.MyString();
	}
	helloWorld (){
		helloWorld = "Hello World";
		// TODO Auto-generated method stub
	}
	
	public void MyString() {
		System.out.print(helloWorld);
	}

}
